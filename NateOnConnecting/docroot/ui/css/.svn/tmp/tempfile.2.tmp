.replyPaging { clear:both; position:relative; bottom:-12px; padding:0 7px 0 0; text-align:right; }  
	.replyPaging span.paging { display:inline-block; margin:0 4px }
	.replyPaging a { margin:0 2px; font-family:Tahoma; font-size:9px; color:#666; text-decoration:none; }
	.replyPaging a.current { color:#fd0303; cursor:default;  font-weight:bold; }
	.replyPaging a.btn { margin:0 0 0 -3px; .margin:1px 0 0 1px; display:inline-block; background:url('../img/bg_innerpg.gif') no-repeat; height:10px; overflow:hidden; text-indent:-1000em; vertical-align:middle; }
	.replyPaging a.btn.first_page { width:12px; background-position:0 0; } 
	.replyPaging a.btn.prev_page { width:9px; background-position:-13px 0; } 
	.replyPaging a.btn.next_page { width:9px; background-position:-23px 0; } 
	.replyPaging a.btn.last_page { width:12px; background-position:-33px 0; } 

