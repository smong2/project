	.pf_mng { display:none; position:absolute; top:38px; left:5px; width:47px; height:16px; background:url('../img/btn_manage.gif') no-repeat; overflow:hidden; text-indent:-5000em; z-index:2; border:none !important; }
	
.cntAdd { position:relative; text-align:right; padding-bottom:1px;}
	.cntAdd a { display:inline-block; background:url('../img/btn.gif') right -22px no-repeat; letter-spacing:-1px; padding-right:12px; }

.tabConnect { position:relative; top:81px; height:33px; background:url('../img/bg_tab.gif') repeat-x; }
