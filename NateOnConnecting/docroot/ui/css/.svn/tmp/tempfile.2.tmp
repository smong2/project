	.tabConnect .btnRefresh { position:absolute; top:0; right:0; width:10px; height:33px; background:url('../img/bg_tabright.gif') right top no-repeat; text-indent:-5000em; overflow:hidden; } 

.title_cntingList { position:relative; padding:7px 0 0 0; z-index:3; }
.title_cntingList .sel_view { display:block; border:1px solid #dfdfdf; width:124px; height:11px; padding:5px 10px; background:url('../img/arrow01.gif') 127px 4px no-repeat; letter-spacing:-1px; margin-right:27px; }
.title_cntingList .layer_select { position:absolute; top:7px; left:0; border:1px solid #dfdfdf; width:124px; height:90px; padding:5px 10px; background:#FFF url('../img/arrow01.gif') 127px 4px no-repeat; z-index:1; letter-spacing:-1px; z-index:3; }
