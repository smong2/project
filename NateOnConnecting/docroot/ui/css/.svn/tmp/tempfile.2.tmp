@charset "utf-8";

h1 { display:none; }

.wrap { padding:5px 2px 0 5px; font:11px "돋움", dotum; color:#666; line-height:1.3; min-width:220px; }
.iwrap { font:11px "돋움", dotum; color:#666; line-height:1.3; min-width:220px; padding:0 2px; }

#cnt_connecting { position:relative; height:114px; }

