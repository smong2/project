.title_cntingList .layer_select dd a:hover { color:#fd0303; }

.cntingList { width:100%; _width:99% }
.cntingList li { clear:both; float:left; width:100%; border-bottom:1px solid #EFEFEF; padding:10px 0 0 0; }
.cntingList li a.link { color:#53AEA8; letter-spacing:0; }
.pfImg { position:absolute; top:0; left:-70px; width:70px; }
